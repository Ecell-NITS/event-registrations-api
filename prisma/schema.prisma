generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model EventApplication {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  name       String
  email      String   @unique
  phone      String
  college    String
  ideaBrief  String
  teamName   String
  createdAt  DateTime @default(now())
}

model TreasureApplication {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  email     String   @unique
  phone     String
  college   String
  ideaBrief String
  teamName  String
  createdAt DateTime @default(now())
}

